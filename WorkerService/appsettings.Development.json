{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Redis": {
    "Uri": "redis:6379"
  },
  "RabbitMQ": {
    "Uri": "rabbitmq:5672"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Properties": {
      "Application": "WorkerService"
    }
  },
  "ElasticConfiguration": {
    "Uri": "elasticsearch:9200"
  }
}